{
	"searchFormTable": {
		"prefix": "mk-searchFormTable",
		"body": [
            "<template>"
            "    <SearchFormTable"
            "        ref='searchTable'"
            "        :url='tableParams.url'"
            "        :columns='tableParams.columns'"
            "        :resDataFormat='resDataFormat'"
            "        :formOptions='tableParams.formOptions'"
            "        :params='tableParams.params'>"

            "        <template slot='tablePrepend'>"
            "             <el-button type='primary'> + 新建权限分配记录</el-button>"
            "        </template>"
            "        <template slot='fixed' slot-scope='{}'>"
            "            浮动类型"
            "        </template>"
            "    </SearchFormTable>"
            "</template>"

            "<script>"
            "import { SearchFormTable } from '@mk/vue-element-components'"

            "export default {"
            "    components: {"
            "        SearchFormTable"
            "    },"
            "    data() {"
            "        const options = ["
            "             {"
            "                label: 'hello',"
            "                value: 'hello'"
            "             },"
            "             {"
            "                label: '你好',"
            "                value: '你好'"
            "            }"
            "        ];"
            "        return {"
            "            tableParams: {"
            "                url: 'http://yapi.mockuai.com/mock/397/order/list',"
            "                params: {},"
            "                formOptions: {"
            "                    labelWidth: '72px',"
            "                    size: 'small',"
            "                    forms: ["
            "                        {"
            "                            prop: 'shop_name',"
            "                            label: '手机号码:'"
            "                        },"
            "                        {"
            "                            options: options,"
            "                            prop: 'level',"
            "                            itemType: 'select',"
            "                            label: 'APP: '"
            "                        }"
            "                    ]"
            "             },"
            "            columns: ["
            "                {"
            "                    prop: 'item_name',"
            "                    label: '提交人',"
            "                    showOverflowTooltip: true"
            "                },"
            "                {"
            "                    prop: 'delivery_fee',"
            "                    label: '手机号码',"
            "                    formatType: 'price',"
            "                    showOverflowTooltip: true"
            "                },"
            "                {"
            "                    prop: 'delivery_fee',"
            "                    label: '所在APP',"
            "                    render: row => {"
            "                        return '自定义' + row.consignee;"
            "                    },"
            "                    showOverflowTooltip: true"
            "                },"
            "                {"
            "                    prop: 'item_name',"
            "                    label: '消息类型',"
            "                    showOverflowTooltip: true"
            "                },"
            "                {"
            "                    prop: 'item_name',"
            "                    label: '对象用户',"
            "                    showOverflowTooltip: true"
            "                },"
            "                {"
            "                    slot: 'fixed',"
            "                    label: '操作',"
            "                    fixed: 'right',"
            "                    showOverflowTooltip: true"
            "                }"
            "            ]"
            "        }"
            "    };"
            "},"
            "methods: {"
            "    resDataFormat(res) {"
            "        // 返回数据列表"
            "        return res.data.order_list || [];"
            "    }"
            "}"
            "};"
            "</script>"

            "<style lang='scss' scoped>"
            "</style>"
        ],
        "description":"mk筛选表格组件"
    },
