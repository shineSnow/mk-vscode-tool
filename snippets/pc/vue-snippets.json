{
    "searchFormTable": {
        "prefix": "mk-searchFormTable",
        "body": [
            "<template>",
            "    <SearchFormTable",
            "        ref='searchTable'",
            "        :url='tableParams.url'",
            "        :columns='tableParams.columns'",
            "        :resDataFormat='resDataFormat'",
            "        :formOptions='tableParams.formOptions'",
            "        :params='tableParams.params'>",
            "        <template slot='tablePrepend'>",
            "             <el-button type='primary'> + 新建权限分配记录</el-button>",
            "        </template>",
            "        <template slot='fixed' slot-scope='{}'>",
            "            浮动类型",
            "        </template>",
            "    </SearchFormTable>",
            "</template>",
            "<script>",
            "import { SearchFormTable } from '@mk/vue-element-components'",
            "export default {",
            "    components: {",
            "        SearchFormTable",
            "    },",
            "    data() {",
            "        const options = [",
            "             {",
            "                label: 'hello',",
            "                value: 'hello'",
            "             },",
            "             {",
            "                label: '你好',",
            "                value: '你好'",
            "            }",
            "        ];",
            "        return {",
            "            tableParams: {",
            "                url: 'http://yapi.mockuai.com/mock/397/order/list',",
            "                params: {},",
            "                formOptions: {",
            "                    labelWidth: '72px',",
            "                    size: 'small',",
            "                    forms: [",
            "                        {",
            "                            prop: 'shop_name',",
            "                            label: '手机号码:'",
            "                        },",
            "                        {",
            "                            options: options,",
            "                            prop: 'level',",
            "                            itemType: 'select',",
            "                            label: 'APP: '",
            "                        }",
            "                    ]",
            "             },",
            "            columns: [",
            "                {",
            "                    prop: 'item_name',",
            "                    label: '提交人',",
            "                    showOverflowTooltip: true",
            "                },",
            "                {",
            "                    prop: 'delivery_fee',",
            "                    label: '手机号码',",
            "                    formatType: 'price',",
            "                    showOverflowTooltip: true",
            "                },",
            "                {",
            "                    prop: 'delivery_fee',",
            "                    label: '所在APP',",
            "                    render: row => {",
            "                        return '自定义' + row.consignee;",
            "                    },",
            "                    showOverflowTooltip: true",
            "                },",
            "                {",
            "                    prop: 'item_name',",
            "                    label: '消息类型',",
            "                    showOverflowTooltip: true",
            "                },",
            "                {",
            "                    prop: 'item_name',",
            "                    label: '对象用户',",
            "                    showOverflowTooltip: true",
            "                },",
            "                {",
            "                    slot: 'fixed',",
            "                    label: '操作',",
            "                    fixed: 'right',",
            "                    showOverflowTooltip: true",
            "                }",
            "            ]",
            "        }",
            "    };",
            "},",
            "methods: {",
            "    resDataFormat(res) {",
            "        // 返回数据列表",
            "        return res.data.order_list || [];",
            "    }",
            "}",
            "};",
            "</script>",
            "<style lang='scss' scoped>",
            "</style>"
        ],
        "description": "魔筷筛选表格组件"
    },
    "formTable": {
        "prefix": "mk-formTable",
        "body": [
            "<template>",
            "    <FormTable",
            "        ref=\"formTable\"",
            "        :url=\"tables.url\"",
            "        :resDataFormat=\"resDataFormat\"",
            "        :formModel=\"tables.formModel\"",
            "        :tableModel=\"tables.tableModel\"",
            "    >",
            "        <template slot=\"custom\">",
            "           <el-input v-model=\"tables.formModel.params.custom\"></el-input>",
            "           </template>",
            "       <template slot=\"myBtn\">",
            "           <el-button>345678</el-button>",
            "       </template>",
            "   </FormTable>",
            "</template>",
            "<script>",
            "import { FormTable } from \"@mk/vue-element-components\";",
            "export default {",
            "   components: {",
            "       FormTable",
            "   },",
            "   data() {",
            "       const options = [",
            "           {",
            "               label: 'hello',",
            "               value: 'hello'",
            "           },",
            "           {",
            "               label: '你好',",
            "               value: '你好'",
            "           }",
            "       ]",
            "       return {",
            "           tables: {",
            "               url: 'http://yapi.mockuai.com/mock/397/order/list',",
            "               formModel: {",
            "               formEvent: {},",
            "               formAttr: {},",
            "               params: {},",
            "               forms: [",
            "                   {",
            "                       itemEvent: {},",
            "                       itemAttr: {",
            "                           prop: 'shop_name',",
            "                           label: '输入框',",
            "                       },",
            "                       event: {},",
            "                       attr: {}",
            "                   },",
            "                   {",
            "                       prop: 'last_use_time',",
            "                       label: '时间段',",
            "                       itemType: 'date-picker',",
            "                       type: 'datetimerange',",
            "                       keys: ['last_use_time_start', 'last_use_time_end'],",
            "                       format: ['time'],",
            "                   },",
            "                   {",
            "                       options: options,",
            "                       prop: 'level',",
            "                       itemType: 'select',",
            "                       label: '选择框'",
            "                   },",
            "                   {",
            "                       slot: 'custom',",
            "                       label: '我是slot'",
            "                   },",
            "                   {",
            "                       slot: 'btn'",
            "                   },",
            "                   {",
            "                       slot: 'myBtn',",
            "                   }",
            "               ]",
            "            },",
            "            tableModel: {",
            "               tableAttr: {},",
            "               tableEvent: {},",
            "               columns: [",
            "                   {",
            "                       itemAttr: {",
            "                           prop: 'item_name',",
            "                           label: '类型',",
            "                       },",
            "                       itemEvent: {},",
            "                   },",
            "                   {",
            "                       prop: 'delivery_fee',",
            "                       label: '金额类型',",
            "                       format: ['price'],",
            "                   },",
            "                   {",
            "                       prop: 'xxxx',",
            "                       label: '自定义类型',",
            "                       render: row => {",
            "                           const h = this.$createElement",
            "                           return h(",
            "                               'div',",
            "                               {",
            "                                   style: { color: '#409eff',cursor:'pointer' },",
            "                                   on: { click: this.toPage }",
            "                               },",
            "                               row.item_name",
            "                           )",
            "                       },",
            "                   }",
            "               ]",
            "            }",
            "        }",
            "        }",
            "    },",
            "    methods: {",
            "        toPage() {",
            "           console.log('to page')",
            "        },",
            "        resDataFormat(res) {",
            "           // 返回数据列表",
            "           return res.data.order_list || [];",
            "        },",
            "    },",
            "}"
        ],
        "description": "魔筷列表组件(新版本)"
    },
    "dialogComponent": {
        "prefix": "mk-dialog-component",
        "body": [
            "<template>",
            "    <div class='wrapper'>",
            "        <el-dialog",
            "            title='提示'",
            "            :visible.sync='dialogVisible'",
            "            @open='handleOpen'",
            "            @close='handleClose'>",
            "            <div class='dialog-con'>这是一段信息</div>",
            "            <div slot='footer' class='dialog-footer'>",
            "                <el-button @click='handleCancel'>取 消</el-button>",
            "                <el-button type='primary' @click='handleSubmit'>确 定<el-button>",
            "            </div>",
            "        </el-dialog>",
            "    </div>",
            "</template>",
            "<script>",
            "export default {",
            "    name: 'dialog-add-auth',",
            "    data() {",
            "        return {",
            "            dialogVisible: true",
            "        };",
            "    },",
            "    methods: {",
            "        //弹框关闭回调",
            "        handleClose(done) {",
            "        },",
            "         //弹框打开回调",
            "        handleOpen() {",
            "        },",
            "        //按钮取消回调",
            "        handleCancel() {",
            "        },",
            "        //按钮确定回调",
            "        handleSubmit() {",
            "        }",
            "    }",
            "};",
            "</script>",
            "<style lang='scss' scoped>",
            "</style>"
        ],
        "description": "魔筷弹框组件"
    },
    "dialogForm": {
        "prefix": "mk-dialogForm",
        "body": [
            "<template>",
            "    <div class='wrapper'>",
            "        <el-dialog",
            "            title='提示'",
            "            :visible.sync='dialogVisible'",
            "            @open='handleOpen'",
            "            @close='handleClose'>",
            "                <div class='dialog-con'>",
            "                    <el-form :model='ruleForm' :rules='rules' ref='ruleForm' label-width='100px' class='demo-ruleForm'>",
            "                        <el-form-item label='活动名称' prop='name'>",
            "                            <el-input v-model='ruleForm.name'></el-input>",
            "                        </el-form-item>",
            "                        <el-form-item label='活动区域' prop='region'>",
            "                            <el-select v-model='ruleForm.region' placeholder='请选择活动区域'>",
            "                                <el-option label='区域一' value='shanghai'></el-option>",
            "                                <el-option label='区域二' value='beijing'></el-option>",
            "                            </el-select>",
            "                        </el-form-item>",
            "                        <el-form-item label='活动时间' required>",
            "                            <el-col :span='11'>",
            "                                <el-form-item prop='date1'>",
            "                                    <el-date-picker type='date' placeholder='选择日期' v-model='ruleForm.date1' style='width: 100%;'></el-date-picker>",
            "                                </el-form-item>",
            "                            </el-col>",
            "                            <el-col class='line' :span='2'>-</el-col>",
            "                            <el-col :span='11'>",
            "                        <el-form-item prop='date2'>",
            "                        <el-time-picker placeholder='选择时间' v-model='ruleForm.date2' style='width: 100%;'></el-time-picker>",
            "                        </el-form-item>",
            "                    </el-col>",
            "                        </el-form-item>",
            "                        <el-form-item label='即时配送' prop='delivery'>",
            "                            <el-switch v-model='ruleForm.delivery'></el-switch>",
            "                        </el-form-item>",
            "                        <el-form-item label='活动性质' prop='type'>",
            "                            <el-checkbox-group v-model='ruleForm.type'>",
            "                                <el-checkbox label='美食/餐厅线上活动' name='type'></el-checkbox>",
            "                                <el-checkbox label='地推活动' name='type'></el-checkbox>",
            "                                <el-checkbox label='线下主题活动' name='type'></el-checkbox>",
            "                                <el-checkbox label='单纯品牌曝光' name='type'></el-checkbox>",
            "                            </el-checkbox-group>",
            "                        </el-form-item>",
            "                        <el-form-item label='特殊资源' prop='resource'>",
            "                            <el-radio-group v-model='ruleForm.resource'>",
            "                                <el-radio label='线上品牌商赞助'></el-radio>",
            "                                <el-radio label='线下场地免费'></el-radio>",
            "                            </el-radio-group>",
            "                        </el-form-item>",
            "                        <el-form-item label='活动形式' prop='desc'>",
            "                            <el-input type='textarea' v-model='ruleForm.desc'></el-input>",
            "                        </el-form-item>",
            "                        <el-form-item>",
            "                            <el-button type='primary' @click=\"submitForm('ruleForm')\">立即创建</el-button>",
            "                            <el-button @click=\"resetForm('ruleForm')\">重置</el-button>",
            "                        </el-form-item>",
            "            </el-form>",
            "            </div>",
            "            <div slot='footer' class='dialog-footer'>",
            "            <el-button @click='handleCancel'>取 消</el-button>",
            "                        <el-button type='primary' @click='handleSubmit'>确 定</el-button>",
            "            </div>",
            "        </el-dialog>",
            "    </div>",
            "</template>",
            "<script>",
            "export default {",
            "    name: 'dialog-add-auth',",
            "    data() {",
            "        return {",
            "            dialogVisible: true,",
            "            ruleForm: {",
            "            name: '',",
            "            region: '',",
            "            date1: '',",
            "            date2: '',",
            "            delivery: false,",
            "            type: [],",
            "            resource: '',",
            "            desc: ''",
            "        },",
            "        rules: {",
            "            name: [",
            "            {     required: true, message: '请输入活动名称', trigger: 'blur' },",
            "                { min: 3, max: 5, message: '长度在 3 到 5 个字符', trigger: 'blur' }",
            "            ],",
            "            region: [",
            "                { required: true, message: '请选择活动区域', trigger: 'change' }",
            "            ],",
            "            date1: [",
            "                { type: 'date', required: true, message: '请选择日期', trigger: 'change' }",
            "            ],",
            "            date2: [",
            "                { type: 'date', required: true, message: '请选择时间', trigger: 'change' }",
            "            ],",
            "            type: [",
            "                { type: 'array', required: true, message: '请至少选择一个活动性质', trigger: 'change' }",
            "            ],",
            "            resource: [",
            "                { required: true, message: '请选择活动资源', trigger: 'change' }",
            "            ],",
            "            desc: [",
            "                { required: true, message: '请填写活动形式', trigger: 'blur' }",
            "            ]",
            "        }",
            "        };",
            "    },",
            "    methods: {",
            "        //弹框关闭回调",
            "        handleClose(done) {",
            "        },",
            "        //弹框打开回调",
            "        handleOpen() {",
            "        },",
            "        //按钮取消回调",
            "        handleCancel() {",
            "        },",
            "        //按钮确定回调",
            "        handleSubmit() {",
            "        },",
            "        submitForm(formName) {",
            "            this.$refs[formName].validate((valid) => {",
            "            if (valid) {",
            "                alert('submit!');",
            "            } else {",
            "                console.log('error submit!!');",
            "                return false;",
            "            }",
            "            });",
            "        },",
            "        resetForm(formName) {",
            "            this.$refs[formName].resetFields();",
            "        }",
            "    }",
            "};",
            "</script>",
            "<style lang='scss' scoped>",
            "</style>"
        ],
        "description": "魔筷表单弹框组件"
    }
}